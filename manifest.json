{
    "name": "Keep Notes",
    "manifest_version": 3,
    "version": "0.0.1",

    "action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
        
    },
    
    "icons":{
        "128": "logo.png"
    },
    "background":{
        "service_worker": "background.js"
    },

    "permissions": [
        "storage",
        "tabs",
        "notifications"
    ]
}